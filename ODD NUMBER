package Day4;

import java.util.Scanner;

public class odd {

	public static void main(String[] args) {
				Scanner sc=new Scanner(System.in);
				int a=sc.nextInt();
				int b=sc.nextInt();
				int oddcount=0;
				for(int i=a;i<=b;i++)
				{
					if(i%2!=0)
					{
						oddcount++;
					}
					System.out.println(oddcount);
					
				}
			}
		}
